


<% if flash[:error] %>
  <% flash[:error].each do |e| %>
  <%= e %>
  <% end %>
<% end %>

<%= form_for @recipe do |f| %>
  <p> <%= f.label "Title" %> </p>
  <%= f.text_field :title %>
  <p> <%= f.label "Description" %> </p>
  <%= f.text_area :description %>
  <%= f.submit "Submit Recipe"%>
<% end %>
